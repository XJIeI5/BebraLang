#cinc "<stdio.h>";

fib fn(n u64) u64 := {
    a, c u64;
    b u64 := 1;
    for i u64; <i n @lt>; <i++> {
        c = b;
        b = <a b +>;
        a = c;
    }
    ret a;
}

show_fib fn() := {
    >printf("Fibonacci series!\n");

    >printf("N:\t");
    for i u64; <i 10 @nlt>; <i++> {
        >printf("%llu\t", i);
    }
    >printf("\n");

    >printf("V:\t");
    for i u64; <i 10 @nlt>; <i++> {
        >printf("%llu\t", fib(i));
    }
    >printf("\n");
}

main fn() := {
    show_fib();
}